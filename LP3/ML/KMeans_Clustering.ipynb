    "def returnCluster(m1,m2,x_co,y_co):\n",
    "    distance1=eucledian_distance(m1[0],m1[1],x_co,y_co)\n",
    "    \n",
    "    distance2=eucledian_distance(m2[0],m2[1],x_co,y_co)\n",
    "    \n",
    "    if(distance1<distance2):\n",
    "        return 1;\n",
    "    else:\n",
    "        return 2;\n",
    "    "
